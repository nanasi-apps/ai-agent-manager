{
  "name": "agent-manager-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter \"@agent-manager/shared\" --filter \"@agent-manager/web\" --filter \"@agent-manager/electron\" --parallel run dev",
    "typecheck": "pnpm -r run typecheck",
    "build": "pnpm -r run build",
    "lint": "biome lint .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10"
  }
}